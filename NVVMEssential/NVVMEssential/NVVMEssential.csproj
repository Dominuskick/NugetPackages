<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>MVVMNavigate</PackageId>
    <Authors>Dominuskick</Authors>
    <Description>Navigation logic between viewmodels</Description>
    <PackageTags>Navigate logic between viewmodels</PackageTags>
    <PackageProjectUrl>https://github.com/Dominuskick/NugetPackages</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Dominuskick/NugetPackages</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

</Project>
